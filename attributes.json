{
   "run_list": [

   "recipe[apt]",
   "recipe[build-essential]",
   "recipe[curl]",
   "recipe[git]",
   "recipe[imagemagick]",
   "recipe[libqt4::dev]",
   "recipe[libreoffice]",
   "recipe[libsasl2_dev]",
   "recipe[memcached]",
   "recipe[nodejs]",
   "recipe[openssl]",
   "recipe[passenger_nginx]",
   "recipe[phantomjs]",
   "recipe[python]",
   "recipe[ruby_dev]",
   "recipe[rvm::system]",
   "recipe[yum]",
   "recipe[java]",
   "recipe[java::default_java_symlink]",
   "recipe[elasticsearch]",
   "recipe[pdftk]",
   "recipe[freetds]",
   "recipe[libmagic]",
   "recipe[firefox]",
   "recipe[xvfb]",
   "recipe[redisio]",
   "recipe[redisio::enable]"

  ],
  "apt" : {
    "compile_time_update" : true
  },
  "java" : {
    "install_flavor" : "oracle",
    "jdk_version" : "8",
    "oracle" : {"accept_oracle_download_terms" : true}
  },
  "elasticsearch" : {
    "allocated_memory": "1024m",
    "cluster": {"name": "answers"},
    "custom_config": {"script.disable_dynamic": true},
    "version": "1.3.4"
  },
  "postgresql" : {
    "version" : "9.3",
    "apt_distribution" : "trusty",
    "conf" : {
      "listen_addresses" : "*",
      "log_error_verbosity" : "verbose",
      "log_connections" : "on",
      "log_min_error_statement" : "info",
      "log_min_duration_statement" : "0"
    },
    "pg_hba" : [
      { "type" : "local", "db" : "all", "user" : "postgres", "addr" : null, "method" : "ident"},
      { "type" : "local", "db" : "all", "user" : "all", "addr" : null, "method" : "ident"},
      { "type" : "host", "db" : "all", "user" : "all", "addr" : "0.0.0.0/0", "method" : "md5"},
      { "type" : "host", "db" : "all", "user" : "all", "addr" : "::1/128", "method" : "md5"},
      { "type" : "local", "db" : "all", "user" : "all", "addr" : null, "method" : "ident"}
    ]
  },
  "rvm" : {
    "default_ruby" : "ruby-2.1.5",
    "global_gems" : [
      { "name" : "bundler" },
      { "name" : "rails", "version" : "4.1.9" }
    ]
  },
  "dynamodb-local" : {
    "user" : "root"
  },
  "nodejs" : {
    "npm_packages" : [
      {
        "name" : "istanbul"
      }
    ]
  }
}
